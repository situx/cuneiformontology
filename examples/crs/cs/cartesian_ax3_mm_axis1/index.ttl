@prefix ns1: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix ns2: <http://www.opengis.net/ont/crs/> .
@prefix ns3: <http://situx.github.io/proj4rdf/ont/crs/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis1> a ns3:CoordinateSystemAxis,
        ns2:CoordinateSystemAxis ;
    rdfs:label "Cartesian coordinate system with 3 axis in millimetre units: Axis 1"@en ;
    ns3:abbreviation "X" ;
    ns3:axisDirection ns3:geocentricX ;
    ns3:axisOrder "1"^^xsd:int ;
    ns1:hasUnit ns1:millimetre ;
    ns2:abbreviation "X" ;
    ns2:axisDirection ns2:geocentricX ;
    ns2:axisOrder "1"^^xsd:int .

