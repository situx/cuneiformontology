@prefix ns1: <http://www.opengis.net/ont/crs/> .
@prefix ns2: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix ns3: <http://situx.github.io/proj4rdf/ont/crs/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis2> a ns3:CoordinateSystemAxis,
        ns1:CoordinateSystemAxis ;
    rdfs:label "Cartesian coordinate system with 3 axis in millimetre units: Axis 2"@en ;
    ns3:abbreviation "Y" ;
    ns3:axisDirection ns3:geocentricY ;
    ns3:axisOrder "2"^^xsd:int ;
    ns2:hasUnit ns2:millimetre ;
    ns1:abbreviation "Y" ;
    ns1:axisDirection ns1:geocentricY ;
    ns1:axisOrder "2"^^xsd:int .

