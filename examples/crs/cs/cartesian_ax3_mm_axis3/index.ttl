@prefix ns1: <http://situx.github.io/proj4rdf/ont/crs/> .
@prefix ns2: <http://www.opengis.net/ont/crs/> .
@prefix ns3: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis3> a ns1:CoordinateSystemAxis,
        ns2:CoordinateSystemAxis ;
    rdfs:label "Cartesian coordinate system with 3 axis in millimetre units: Axis 3"@en ;
    ns1:abbreviation "Z" ;
    ns1:axisDirection ns1:geocentricZ ;
    ns1:axisOrder "3"^^xsd:int ;
    ns3:hasUnit ns3:millimetre ;
    ns2:abbreviation "Z" ;
    ns2:axisDirection ns2:geocentricZ ;
    ns2:axisOrder "3"^^xsd:int .

