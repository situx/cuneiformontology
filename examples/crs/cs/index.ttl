@prefix ns1: <http://www.opengis.net/ont/crs/> .
@prefix ns2: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix ns3: <https://situx.github.io/proj4rdf/ont/crs/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm> a ns1:CartesianCS,
        ns3:CartesianCS ;
    rdfs:label "Cartesian coordinate system with 3 axis in millimetre units"@en ;
    ns1:asSVG "<svg width='400' height='250' viewbox='0 0 375 220'><defs><marker id='arrowhead' markerWidth='10' markerHeight='7' refX='0' refY='2' orient='auto'><polygon points='0 0, 4 2, 0 4' /></marker></defs><line x1='20' y1='200' x2='200' y2='200' stroke='red' stroke-width='5' marker-end='url(#arrowhead)'></line><text x='110' y='220' class='small'>X: Cartesian X Axis (om:millimetre)</text><line x1='20' y1='200' x2='20' y2='20' stroke='green' stroke-width='5' marker-end='url(#arrowhead)'></line><text x='35' y='20' class='small'>Y: Cartesian Y Axis (om:millimetre)</text><line x1='20' y1='200' x2='190' y2='30' stroke='blue' stroke-width='5' marker-end='url(#arrowhead)'></line><text x='210' y='25' class='small'>Z: Cartesian Z Axis (om:millimetre)</text></svg>"^^ns1:svgLiteral ;
    ns1:asWKT "CS[\"cartesian\",3],AXIS[\"X\", \"geocentricX\", ORDER[1],            LENGTHUNIT[\"millimetre\",1]        ],        AXIS[\"Y\", \"geocentricY\", ORDER[2],            LENGTHUNIT[\"millimetre\",1]        ],        AXIS[\"Z\", \"geocentricZ\", ORDER[3],            LENGTHUNIT[\"millimetre\",1]        ]]"^^<http://www.opengis.net/ont/geosparql/crs#wktLiteral> ;
    ns1:axis <https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis1>,
        <https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis2>,
        <https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis3> ;
    ns3:asSVG "<svg width='400' height='250' viewbox='0 0 375 220'><defs><marker id='arrowhead' markerWidth='10' markerHeight='7' refX='0' refY='2' orient='auto'><polygon points='0 0, 4 2, 0 4' /></marker></defs><line x1='20' y1='200' x2='200' y2='200' stroke='red' stroke-width='5' marker-end='url(#arrowhead)'></line><text x='110' y='220' class='small'>X: Cartesian X Axis (om:millimetre)</text><line x1='20' y1='200' x2='20' y2='20' stroke='green' stroke-width='5' marker-end='url(#arrowhead)'></line><text x='35' y='20' class='small'>Y: Cartesian Y Axis (om:millimetre)</text><line x1='20' y1='200' x2='190' y2='30' stroke='blue' stroke-width='5' marker-end='url(#arrowhead)'></line><text x='210' y='25' class='small'>Z: Cartesian Z Axis (om:millimetre)</text></svg>"^^xsd:string ;
    ns3:asWKT "CS[\"cartesian\",3],AXIS[\"X\", \"geocentricX\", ORDER[1],            LENGTHUNIT[\"millimetre\",1]        ],        AXIS[\"Y\", \"geocentricY\", ORDER[2],            LENGTHUNIT[\"millimetre\",1]        ],        AXIS[\"Z\", \"geocentricZ\", ORDER[3],            LENGTHUNIT[\"millimetre\",1]        ]]"^^<http://www.opengis.net/ont/geosparql/crs#wktLiteral> ;
    ns3:axis <https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis1>,
        <https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis2>,
        <https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis3> .

<https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis1> a ns1:CoordinateSystemAxis,
        ns3:CoordinateSystemAxis ;
    rdfs:label "Cartesian coordinate system with 3 axis in millimetre units: Axis 1"@en ;
    ns2:hasUnit ns2:millimetre ;
    ns1:abbreviation "X" ;
    ns1:axisDirection ns1:geocentricX ;
    ns1:axisOrder "1"^^xsd:int ;
    ns3:abbreviation "X" ;
    ns3:asSVG "<svg width=\"400\" height=\"100\" viewbox=\"0 0 275 100\"><defs><marker id=\"arrowhead\" markerWidth=\"10\" markerHeight=\"7\" refX=\"0\" refY=\"2\" orient=\"auto\"><polygon points=\"0 0, 4 2, 0 4\" /></marker></defs><line x1=\"20\" y1=\"50\" x2=\"200\" y2=\"50\" stroke=\"gray\" stroke-width=\"5\" marker-end=\"url(#arrowhead)\"></line><text x=\"30\" y=\"70\" class=\"small\">X: Cartesian X (om:millimetre)</text></svg>"^^<http://www.opengis.net/ont/geosparql/crs#svgLiteral> ;
    ns3:axisDirection ns3:geocentricX ;
    ns3:axisOrder "1"^^xsd:int .

<https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis2> a ns1:CoordinateSystemAxis,
        ns3:CoordinateSystemAxis ;
    rdfs:label "Cartesian coordinate system with 3 axis in millimetre units: Axis 2"@en ;
    ns2:hasUnit ns2:millimetre ;
    ns1:abbreviation "Y" ;
    ns1:axisDirection ns1:geocentricY ;
    ns1:axisOrder "2"^^xsd:int ;
    ns3:abbreviation "Y" ;
    ns3:asSVG "<svg width=\"400\" height=\"100\" viewbox=\"0 0 275 100\"><defs><marker id=\"arrowhead\" markerWidth=\"10\" markerHeight=\"7\" refX=\"0\" refY=\"2\" orient=\"auto\"><polygon points=\"0 0, 4 2, 0 4\" /></marker></defs><line x1=\"20\" y1=\"50\" x2=\"200\" y2=\"50\" stroke=\"gray\" stroke-width=\"5\" marker-end=\"url(#arrowhead)\"></line><text x=\"30\" y=\"70\" class=\"small\">Y: Cartesian Y (om:millimetre)</text></svg>"^^<http://www.opengis.net/ont/geosparql/crs#svgLiteral> ;
    ns3:axisDirection ns3:geocentricY ;
    ns3:axisOrder "2"^^xsd:int .

<https://situx.github.io/cuneiformontology/examples/crs/cs/cartesian_ax3_mm_axis3> a ns1:CoordinateSystemAxis,
        ns3:CoordinateSystemAxis ;
    rdfs:label "Cartesian coordinate system with 3 axis in millimetre units: Axis 3"@en ;
    ns2:hasUnit ns2:millimetre ;
    ns1:abbreviation "Z" ;
    ns1:axisDirection ns1:geocentricZ ;
    ns1:axisOrder "3"^^xsd:int ;
    ns3:abbreviation "Z" ;
    ns3:asSVG "<svg width=\"400\" height=\"100\" viewbox=\"0 0 275 100\"><defs><marker id=\"arrowhead\" markerWidth=\"10\" markerHeight=\"7\" refX=\"0\" refY=\"2\" orient=\"auto\"><polygon points=\"0 0, 4 2, 0 4\" /></marker></defs><line x1=\"20\" y1=\"50\" x2=\"200\" y2=\"50\" stroke=\"gray\" stroke-width=\"5\" marker-end=\"url(#arrowhead)\"></line><text x=\"30\" y=\"70\" class=\"small\">Z: Cartesian Z (om:millimetre)</text></svg>"^^<http://www.opengis.net/ont/geosparql/crs#svgLiteral> ;
    ns3:axisDirection ns3:geocentricZ ;
    ns3:axisOrder "3"^^xsd:int .

